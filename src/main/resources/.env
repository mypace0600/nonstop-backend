# PostgreSQL Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=nonstop_local
DB_USERNAME=postgres
DB_PASSWORD=password

# Google OAuth2 Credentials
GOOGLE_CLIENT_ID="your-google-client-id"
GOOGLE_CLIENT_SECRET="your-google-client-secret"

# JWT Secret Key (must be at least 32 bytes for HS256)
JWT_SECRET_KEY="your-super-secret-key-that-is-long-enough-for-hs256"

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# Admin Email
ADMIN_EMAIL="your-admin-email@gmail.com"

# Spring Mail (Gmail)
# Note: You might need to create an "App Password" for this to work
MAIL_USERNAME="your-gmail-username@gmail.com"
MAIL_PASSWORD="your-gmail-app-password"
